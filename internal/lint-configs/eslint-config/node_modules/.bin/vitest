#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sunn/code/self/open-oj/open-oj-admin/node_modules/.pnpm/vitest@3.2.3_@types+node@24.0.1_happy-dom@17.6.3_jiti@2.4.2_less@4.3.0_sass@1.89.2_terser@5.42.0_yaml@2.8.0/node_modules/vitest/node_modules:/Users/sunn/code/self/open-oj/open-oj-admin/node_modules/.pnpm/vitest@3.2.3_@types+node@24.0.1_happy-dom@17.6.3_jiti@2.4.2_less@4.3.0_sass@1.89.2_terser@5.42.0_yaml@2.8.0/node_modules:/Users/sunn/code/self/open-oj/open-oj-admin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sunn/code/self/open-oj/open-oj-admin/node_modules/.pnpm/vitest@3.2.3_@types+node@24.0.1_happy-dom@17.6.3_jiti@2.4.2_less@4.3.0_sass@1.89.2_terser@5.42.0_yaml@2.8.0/node_modules/vitest/node_modules:/Users/sunn/code/self/open-oj/open-oj-admin/node_modules/.pnpm/vitest@3.2.3_@types+node@24.0.1_happy-dom@17.6.3_jiti@2.4.2_less@4.3.0_sass@1.89.2_terser@5.42.0_yaml@2.8.0/node_modules:/Users/sunn/code/self/open-oj/open-oj-admin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node_modules/.pnpm/vitest@3.2.3_@types+node@24.0.1_happy-dom@17.6.3_jiti@2.4.2_less@4.3.0_sass@1.89.2_terser@5.42.0_yaml@2.8.0/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../../../../../node_modules/.pnpm/vitest@3.2.3_@types+node@24.0.1_happy-dom@17.6.3_jiti@2.4.2_less@4.3.0_sass@1.89.2_terser@5.42.0_yaml@2.8.0/node_modules/vitest/vitest.mjs" "$@"
fi
