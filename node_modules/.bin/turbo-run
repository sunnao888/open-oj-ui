#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sunn/code/self/open-oj/open-oj-ui/scripts/turbo-run/bin/node_modules:/Users/sunn/code/self/open-oj/open-oj-ui/scripts/turbo-run/node_modules:/Users/sunn/code/self/open-oj/open-oj-ui/scripts/node_modules:/Users/sunn/code/self/open-oj/open-oj-ui/node_modules:/Users/sunn/code/self/open-oj/node_modules:/Users/sunn/code/self/node_modules:/Users/sunn/code/node_modules:/Users/sunn/node_modules:/Users/node_modules:/node_modules:/Users/sunn/code/self/open-oj/open-oj-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sunn/code/self/open-oj/open-oj-ui/scripts/turbo-run/bin/node_modules:/Users/sunn/code/self/open-oj/open-oj-ui/scripts/turbo-run/node_modules:/Users/sunn/code/self/open-oj/open-oj-ui/scripts/node_modules:/Users/sunn/code/self/open-oj/open-oj-ui/node_modules:/Users/sunn/code/self/open-oj/node_modules:/Users/sunn/code/self/node_modules:/Users/sunn/code/node_modules:/Users/sunn/node_modules:/Users/node_modules:/node_modules:/Users/sunn/code/self/open-oj/open-oj-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@vben/turbo-run/bin/turbo-run.mjs" "$@"
else
  exec node  "$basedir/../@vben/turbo-run/bin/turbo-run.mjs" "$@"
fi
