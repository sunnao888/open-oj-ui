#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/code/open-oj-ui/node_modules/.pnpm/depcheck@1.4.7/node_modules/depcheck/bin/node_modules:/mnt/f/code/open-oj-ui/node_modules/.pnpm/depcheck@1.4.7/node_modules/depcheck/node_modules:/mnt/f/code/open-oj-ui/node_modules/.pnpm/depcheck@1.4.7/node_modules:/mnt/f/code/open-oj-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/f/code/open-oj-ui/node_modules/.pnpm/depcheck@1.4.7/node_modules/depcheck/bin/node_modules:/mnt/f/code/open-oj-ui/node_modules/.pnpm/depcheck@1.4.7/node_modules/depcheck/node_modules:/mnt/f/code/open-oj-ui/node_modules/.pnpm/depcheck@1.4.7/node_modules:/mnt/f/code/open-oj-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../depcheck/bin/depcheck.js" "$@"
else
  exec node  "$basedir/../depcheck/bin/depcheck.js" "$@"
fi
