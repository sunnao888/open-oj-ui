#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sunn/code/self/open-oj/open-oj-admin/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.27.0_@types+node@24.0.1_async-validator@4.2.5__d96f9ca8f7da5cb844368511a84f59b5/node_modules/vitepress/bin/node_modules:/Users/sunn/code/self/open-oj/open-oj-admin/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.27.0_@types+node@24.0.1_async-validator@4.2.5__d96f9ca8f7da5cb844368511a84f59b5/node_modules/vitepress/node_modules:/Users/sunn/code/self/open-oj/open-oj-admin/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.27.0_@types+node@24.0.1_async-validator@4.2.5__d96f9ca8f7da5cb844368511a84f59b5/node_modules:/Users/sunn/code/self/open-oj/open-oj-admin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sunn/code/self/open-oj/open-oj-admin/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.27.0_@types+node@24.0.1_async-validator@4.2.5__d96f9ca8f7da5cb844368511a84f59b5/node_modules/vitepress/bin/node_modules:/Users/sunn/code/self/open-oj/open-oj-admin/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.27.0_@types+node@24.0.1_async-validator@4.2.5__d96f9ca8f7da5cb844368511a84f59b5/node_modules/vitepress/node_modules:/Users/sunn/code/self/open-oj/open-oj-admin/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.27.0_@types+node@24.0.1_async-validator@4.2.5__d96f9ca8f7da5cb844368511a84f59b5/node_modules:/Users/sunn/code/self/open-oj/open-oj-admin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
