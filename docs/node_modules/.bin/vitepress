#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sunn/code/self/open-oj/open-oj-ui/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.25.0_@types+node@22.15.18_async-validator@4.2._6db4f4d4b0c601f314f30a3917c23575/node_modules/vitepress/bin/node_modules:/Users/sunn/code/self/open-oj/open-oj-ui/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.25.0_@types+node@22.15.18_async-validator@4.2._6db4f4d4b0c601f314f30a3917c23575/node_modules/vitepress/node_modules:/Users/sunn/code/self/open-oj/open-oj-ui/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.25.0_@types+node@22.15.18_async-validator@4.2._6db4f4d4b0c601f314f30a3917c23575/node_modules:/Users/sunn/code/self/open-oj/open-oj-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sunn/code/self/open-oj/open-oj-ui/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.25.0_@types+node@22.15.18_async-validator@4.2._6db4f4d4b0c601f314f30a3917c23575/node_modules/vitepress/bin/node_modules:/Users/sunn/code/self/open-oj/open-oj-ui/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.25.0_@types+node@22.15.18_async-validator@4.2._6db4f4d4b0c601f314f30a3917c23575/node_modules/vitepress/node_modules:/Users/sunn/code/self/open-oj/open-oj-ui/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.25.0_@types+node@22.15.18_async-validator@4.2._6db4f4d4b0c601f314f30a3917c23575/node_modules:/Users/sunn/code/self/open-oj/open-oj-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
